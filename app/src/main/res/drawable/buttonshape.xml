<?xml version="1.0" encoding="utf-8"?>

<!-- Style des 2 boutons vers les interface patient et thérapeute dans la page d'acceuil -->
<!-- Pour faire des boutons personalisés, se reporter au rapport où un lien est indiqué -->
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
    <corners
        android:radius="20dp"
        />
    <solid
        android:color="#D71201"
        />
    <padding
        android:left="20dp"
        android:top="20dp"
        android:right="20dp"
        android:bottom="20dp"
        />
    <stroke
        android:width="3dp"
        android:color="#eaeaea"
        />
</shape>